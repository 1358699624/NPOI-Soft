<div class="text-center">
 <button type="button" class="layui-btn layui-btn-fluid" title="swaggerapi" onclick="swaggerapiDB()" style="width: 400px;">swaggerapi调用</button>
</div>
<script type="text/javascript">
    function swaggerapiDB() {
        var index = layer.msg('调用中...', { icon: 16, shade: 0.8, time: false });
        $.ajax({
            async: true,
            dataType: "json",
            type: "post",
            url: "@Url.Action("QueryHouseCentersByOwnerMember","SwaggerApi")",
            data:{ // 提交数据
                "gettable": "admin", // 前者为字段名，后者为数据
                "page": 1,
                "limit": 2,
                "userName": "涂登奎"
            },
            success: function (res) {
                layer.close(index);
                if (res.code == 1) {
                    window.location.href = res.data;
                } else {
                    alert(res.data);
                }
            }
        });
    }
</script>